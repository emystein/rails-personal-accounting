<p id="notice"><%= notice %></p>

<h1>Money Transactions</h1>

<table>
  <thead>
    <tr>
      <th>Savings account</th>
      <th>Date</th>
      <th>Direction</th>
      <th>Amount</th>
    </tr>
  </thead>

  <tbody>
    <% @money_transactions.each do |money_transaction| %>
      <tr>
        <td><%= money_transaction.savings_account_id %></td>
        <td><%= money_transaction.created_at %></td>
        <td><%= money_transaction.direction %></td>
        <td><%= money_transaction.amount %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Money Transaction', new_money_transaction_path %>
