<table>
  <thead>
  <tr>
    <th>Date</th>
    <th>Direction</th>
    <th>Amount</th>
  </tr>
  </thead>

  <tbody>
  <% money_transactions.each do |money_transaction| %>
    <tr>
      <td><%= money_transaction.created_at %></td>
      <td><%= money_transaction.direction %></td>
      <td><%= money_transaction.amount %></td>
    </tr>
  <% end %>
  </tbody>
</table>
