<table class="table table-striped width-md">
  <thead class="thead-dark">
  <tr>
    <th>Date</th>
    <th>Amount</th>
    <th>Description</th>
  </tr>
  </thead>

  <tbody>
  <% money_transactions.each do |money_transaction| %>
    <tr>
      <td><%= money_transaction.created_at %></td>
      <td><%= money_transaction.amount %></td>
      <td><%= money_transaction.description %></td>
    </tr>
  <% end %>
  </tbody>
</table>