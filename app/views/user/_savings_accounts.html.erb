<h1>Savings Accounts</h1>

<table>
  <thead>
  <tr>
    <th>Currency</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @savings_accounts.each do |savings_account| %>
    <tr>
      <td><%= savings_account.currency %></td>
      <td><%= link_to 'Deposit', new_deposit_path(savings_account) %></td>
      <td><%= link_to 'Withdraw', new_withdrawal_path(savings_account) %></td>
      <td><%= link_to 'History', savings_account %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>